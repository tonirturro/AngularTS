{
  "compilerOptions": {
    "noImplicitAny": false,
    "module": "commonjs",
    "noEmitOnError": true,
    "removeComments": true,
    "sourceMap": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "target": "es2015"
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
   ]
}
